\mode<presentation>

\definecolor{forange}{rgb}{1,0.8,0}

\setbeamercolor{section in toc}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=forange!80!gray}
\setbeamercolor*{palette primary}{fg=gray,bg=gray!30!white}
\setbeamercolor*{palette secondary}{fg=forange!80!black,bg=gray!15!white}
\setbeamercolor*{palette tertiary}{bg=forange!90!gray,fg=gray}
\setbeamercolor*{palette quaternary}{fg=forange,bg=gray!5!white}

\setbeamercolor*{sidebar}{fg=forange,bg=gray!15!white}

\setbeamercolor*{palette sidebar primary}{fg=forange!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=forange!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=gray!10!white}

%\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{titlelike}{parent=palette primary,fg=black,bg=forange}
\setbeamercolor{frametitle}{bg=forange}
\setbeamercolor{frametitle right}{bg=gray}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
